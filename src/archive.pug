doctype html
html(lang=ja)
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1.0, maximum-scale=1.0')
    include includes/seo
    title #{page_title}
    include includes/bootstrap-css
    link(rel="stylesheet" href="./css/override.css")
  body
    include includes/header
    .container.nav-padding.content
      h1 過去のWISS
      each datum in data
        h2 #{datum["name"]}
        p
          a(href=datum["url"]) ウェブページ
        each image in datum["images"]
          a(href="./assets/originals/" + image)
            img(src="./assets/thumbnails/" + image style="max-width: 320px; max-height: 320px;").img-thumbnail.rounded
    include includes/footer
    include includes/bootstrap-js
